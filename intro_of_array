#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"enter the size of an array:"<<endl;
    cin>>n;
    int arr[n];
    cout<<"enter the element of an array"<<endl;
    for(int i=0;i<=n-1;i++){
        cin>>arr[i];
       
    }
     for(int i=0;i<=n-1;i++){
      cout<<"Element of an array"<<i+1<<"  :"<<arr[i]<<endl;
        
    }
    
}
